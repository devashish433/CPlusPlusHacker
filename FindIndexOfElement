#include <iostream>
using namespace std;

int main() {
    int arr[] = {5, 3, 8, 2, 7};
    int n = sizeof(arr) / sizeof(arr[0]);
    int target = 8;

    for (int i = 0; i < n; i++) {
        if (arr[i] == target) {
            cout << "Element found at index: " << i << endl;
            break;
        }
    }

    return 0;
}
